pipeline {
    agent {
        label params.AGENT_LABEL
    }
    stages {
        stage('Generate Manifest') {
            steps {
                sh "python lib/make_docker_manifest.py -d dockerfiles/official -m sapmachine.manifest --latest ${LATEST_MAJOR}"
                archiveArtifacts 'sapmachine.manifest'
            }
        }
    }
    post {
        always {
            cleanWs deleteDirs: true, disableDeferredWipeout: true
        }
    }
}